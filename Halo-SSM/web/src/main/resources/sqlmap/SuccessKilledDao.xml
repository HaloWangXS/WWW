<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.halo.dao.SuccessKilledDao">
    <!--为DAO接口方法提供sql语句配置-->
    <insert id="insertSuccessKilled">
       insert into success_killed(seckill_id,user_phone,state,buys)
       values (#{secKillId},#{userPhone},0,#{buys})
    </insert>
</mapper>